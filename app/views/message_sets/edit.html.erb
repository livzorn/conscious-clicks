<div class="container-devise-form">
  <h2 >Edit your message_set</h2>

  <div class="add_message_set">
    <div class="container">
      <div class="form-inputs">
        <%= simple_form_for @message_set do |f| %>
          <div class="form-inputs">
            <%= f.input :theme, label: "theme" %>
            <%= f.input :messages, label: "add your messages" %>
            <%= f.input :link, label: "add your links" %>
          </div>

          <div class="form-actions">
            <div class="book">
              <%= f.submit "add your message set", class: "btn-pink"%>
            </div>
          </div>
        <% end %>
      </div>
    </div>
    <%= link_to 'back', root_path, class: "btn-back" %>
  </div>
</div>
